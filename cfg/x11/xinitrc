#!/bin/bash

# Trying this out!
xhost +si:localuser:"$USER"
setxkbmap -option caps:escape

sxhkd &
xcmenu &
$XDG_DATA_HOME/x11/gitbar &
syncthing &
compton -b &
unclutter -grab &
~/local/share/x11/barrun &
~/local/share/x11/statsbar & 
hsetroot -fill ~/media/pictures/calvin1.jpg -tint '#ebdbb2'
exec ~/local/share/x11/watcher 
