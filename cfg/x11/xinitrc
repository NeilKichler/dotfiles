#!/bin/bash

# Trying this out!
xhost +si:localuser:"$USER"

sxhkd &
xcmenu &
syncthing &
compton -b &
unclutter -grab &
~/local/share/x11/barrun &
~/local/share/x11/statsbar & 
redshift -c ~/local/cfg/redshift.conf &
hsetroot -fill ~/media/pictures/cubone.jpg -tint '#222222' -fliph -contrast 0.55
exec ~/local/share/x11/watcher 
