#!/bin/bash

# Trying this out!
xhost +si:localuser:"$USER"

x11fs -o auto_unmount ~/run/x11 &
wait
mpd &
sxhkd &
xcmenu &
compton &
#syncthing &
colortempd -t 4000 &
~/local/cfg/x11fs/bar & 
~/local/cfg/x11fs/barrun &
GMusicProxy --config ~/local/cfg/gmusicproxy/config &
exec ~/local/cfg/x11fs/watcher 
