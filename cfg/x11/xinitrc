#!/bin/bash

mutt &
sxhkd &
dunst &
xcmenu &
syncthing &
newsbeuter &
colortempd -t 4000
~/local/cfg/wmutils/bar & 
~/local/cfg/wmutils/watcher &
compton -Cc --no-fading-openclose &
feh --bg-scale ~/media/backgrounds/$(( $(( RANDOM % $(find ~/media/backgrounds | wc -l) )) + 1 )).png --no-fehbg &
exec swm
