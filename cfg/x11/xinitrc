#!/bin/bash

mutt &
sxhkd &
dunst &
xcmenu &
syncthing &
newsbeuter &
colortempd -t 4000
~/local/cfg/wmutils/bar & 
compton -Cc --no-fading-openclose &
feh --bg-scale ~/media/backgrounds/$(( $(( RANDOM % $(find ~/media/backgrounds | wc -l) )) + 1 )).png --no-fehbg $
x11fs -o auto_unmount ~/run/x11fs &
exec ~/local/cfg/wmutils/watcher
#exec ~/local/cfg/x11fs/watcher 
#exec swm
