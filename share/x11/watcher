#!/usr/bin/env bash
DIR=$XDG_DATA_HOME/x11

wew | while read -r ev; do
  case "$ev" in
    18*)  $DIR/wshuf
          $DIR/groupsbar           ;;
    19*)  $DIR/wshuf
          $DIR/autotag "${ev#*:}"
          wtf "${ev#*:}"
          $DIR/groupsbar           ;;
    17*)  wtf $(lsw | head -n1)
          $DIR/groupsbar           ;;
  esac
done
