#!/bin/sh
# Script to open Vimb instances in tabbed

lsgrp 2 | xargs mapw -u &>/dev/null

## main
ch() {		
		exec chromium --kiosk --user-data-dir=/home/halfwit/local/cfg/chromium --window-size=800,600 "$@"
}


case "$@" in
	*reddit*)	exec st -e rtv -l "$@" ;;
	*cxcore*)	exec surf "$@"         ;;		
	*)			ch "$@"                ;; 
esac

