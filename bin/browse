#!/usr/bin/env bash
# Script to open Vimb instances in tabbed

## main
lsgrp 2 | xargs mapw -u

#[[ $@ == *reddit* ]] && exec st -t browse -e elinks "$@"
qt() {
  exec qutebrowser --backend webengine "$@"
}

case "$@" in
  *facebook*) qt "$@" ;;
  *google*)   qt "$@" ;;
  *github*)   qt "$@" ;; 
  *youtube*)  qt "$@" ;;
esac

exec surf "$@"
