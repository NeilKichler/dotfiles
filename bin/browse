#!/usr/bin/env bash
# Script to open Vimb instances in tabbed

## main
while read -r id; do
  grp browse "$id"
done < <(lsgrp 2)

lsgrp browse | xargs mapw -u 

qt() {
  exec qutebrowser --backend webengine --target window "$@"
}

case "$@" in
  *facebook*) qt "$@" ;;
  *google*)   qt "$@" ;;
  *github*)   qt "$@" ;; 
  *youtube*)  qt "$@" ;;
  *kijiji*)   qt "$@" ;;
  *reddit*)   qt "$@" ;;
esac

exec surf "$@"
