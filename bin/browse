#!/usr/bin/env bash
# Script to open Vimb instances in tabbed

lsgrp 2 | xargs mapw -u

## main
ch() {
  exec chromium --kiosk "$@"
}

case "$@" in
  
  *google*)   ch "$@" ;;
#  *github*)   ch "$@" ;; 
  *youtube*)  ch "$@" ;;
#  *nixers*)   ch "$@" ;;
#  *kijiji*)   ch "$@" ;;
#  *reddit*)   ch "$@" ;;
  *!*)        ch "$@" ;;
esac

exec surf "$@"
